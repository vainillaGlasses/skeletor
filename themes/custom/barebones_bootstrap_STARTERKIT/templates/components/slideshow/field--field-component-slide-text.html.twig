{#
/**
 * @file
 * Hero title field override.
 *
 */
#}
{%
  set classes = [
    'field',
    'field--name-' ~ field_name|clean_class,
    'field--type-' ~ field_type|clean_class,
    'field--label-' ~ label_display,
    'component',
    'component--slideshow',
    'full-width',
    'slideshow',
  ]
%}
{% for item in items %}
  <p class='myText' {{ attributes.addClass(classes) }}>{{ item.content }}</p>
{% endfor %}
